    "cz-conventional-changelog": "2.1.0",
    "espree": "5.0.0",
    "esutils": "2.0.2",
    "grunt": "1.0.3",
    "grunt-cli": "1.2.0",
    "grunt-eslint": "21.0.0",
    "istanbul": "0.4.5",
    "mocha": "5.2.0",
    "standard-version": "git+https://github.com/jthomerson/standard-version.git#fix-305-header-repeat",
    "typescript": "3.0.3",
    "typescript-eslint-parser": "21.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
